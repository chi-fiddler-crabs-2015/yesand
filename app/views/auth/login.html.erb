<%= form_for :user do |f| %>
  <fieldset>
    <legend>Log In</legend>
    <%= render "shared/errors" %>
    <%= f.label :username %>
    <%= f.text_field :username %><br />

    <%= f.label :password %>
    <%= f.password_field :password %><br />

    <%= f.submit "Log In" %>
  </fieldset>
<% end %>
